{
  "name": "mnmo-monitor",
  "version": "0.1.0",
  "description": "A modern web-application to monitor data.",
  "main": "index.js",
  "browserify-shim": {
    "react": "global:React",
    "react-intl": "global:ReactIntl",
    "flummox": "global:Flummox",
    "../Flux": "global:Flummox",
    "flummox/component": "global:FluxComponent",
    "./shared": "global:MNMOVariables",
    "mnmo-components/lib/themes/mnmo/stage": "global:MNMOStage",
    "mnmo-components/lib/themes/mnmo/centeredbox": "global:MNMOCenteredBox",
    "mnmo-components/lib/themes/mnmo/fieldset": "global:MNMOFieldSet",
    "mnmo-components/lib/themes/mnmo/textinput": "global:MNMOTextInput",
    "mnmo-components/lib/themes/mnmo/select": "global:MNMOSelect",
    "mnmo-components/lib/themes/mnmo/checkbox": "global:MNMOCheckbox",
    "mnmo-components/lib/themes/mnmo/submit": "global:MNMOSubmit",
    "./actions/country": "global:ActionsCountry",
    "./actions/user": "global:ActionsUser",
    "./stores/country": "global:StoresCountry",
    "./stores/user": "global:StoresUser",
    "../../config/entrypoints.json": "global:ConfigEntrypoints"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "font:clean": "rm -rf ./assets/fonts",
    "font:mkdir": "mkdir -p ./assets/fonts",
    "font:download:roboto": "goog-webfont-dl --all -f Roboto",
    "font:mv:roboto": "mv Roboto* ./assets/fonts",
    "font": "npm run font:clean && npm run font:mkdir && npm run font:download:roboto && npm run font:mv:roboto",
    "dev:lib": "npm install -D ../mnmo-components/dist/npm",
    "dev:lint": "eslint .",
    "dev:outdated": "npm outdated --depth=0",
    "dev:clean": "rm -rf ./dist/www",
    "dev:mkdir": "mkdir -p ./dist/www/js && mkdir -p ./dist/www/css && mkdir -p ./dist/www/fonts",
    "dev:js:polyfill": "cat node_modules/es5-shim/es5-shim.js node_modules/es5-shim/es5-sham.js node_modules/console-polyfill/index.js ./node_modules/intl/Intl.js ./node_modules/whatwg-fetch/fetch.js > ./dist/www/js/polyfills.js",
    "dev:js:vendors": "browserify -o ./dist/www/js/vendors.js -r react -r react-intl -r flummox -r 'flummox/component'",
    "dev:js:app": "browserify -o ./dist/www/js/app.js -e src/app.js -x react -x react-intl -x flummox -x 'flummox/component' -t babelify",
    "dev:html": "babel-node ./bin/pageDev.js > ./dist/www/index.html",
    "dev:img:custom": "cp ./assets/custom/dev/bg01.jpg ./dist/www/img/.",
    "dev:img:lib": "cp -R ./node_modules/mnmo-components/lib/themes/mnmo/img ./dist/www",
    "dev:css:normalize": "cp -f ./node_modules/normalize.css/normalize.css ./dist/www/css/.",
    "dev:css:typography": "cp -f ./node_modules/mnmo-components/lib/themes/mnmo/styles/typography.css ./dist/www/css/.",
    "dev:css:app": "cp -f ./src/styles/main.css ./dist/www/css/.",
    "dev:fonts": "cp -f ./assets/fonts/Roboto/Roboto-Bold.ttf ./assets/fonts/Roboto/Roboto-Bold.woff ./assets/fonts/Roboto/Roboto-Light.ttf ./assets/fonts/Roboto/Roboto-Light.woff ./assets/fonts/Roboto/Roboto.eot  ./assets/fonts/Roboto/Roboto.svg  ./assets/fonts/Roboto/Roboto.ttf  ./assets/fonts/Roboto/Roboto.woff ./dist/www/fonts/.",
    "dev:app": "npm run dev:js:app",
    "dev:js": "npm run dev:js:vendors & npm run dev:app & npm run dev:js:polyfill",
    "dev:img": "npm run dev:img:lib && npm run dev:img:custom",
    "dev:css": "npm run dev:css:normalize && npm run dev:css:typography && npm run dev:css:app",
    "predev": "npm run dev:clean && npm run dev:mkdir",
    "dev": "npm run dev:lib && npm run dev:js && npm run dev:img && npm run dev:html && npm run dev:css && npm run dev:fonts",
    "www:clean": "rm -rf ./dist/classic",
    "www:mkdir": "mkdir -p ./dist/classic/js/actions && mkdir -p ./dist/classic/js/stores && mkdir -p ./dist/classic/css && mkdir -p ./dist/classic/lib/js && mkdir -p ./dist/classic/lib/css && mkdir -p ./dist/classic/lib/fonts",
    "www:js:lib:react": "cp -f ./node_modules/react/dist/react.js ./dist/classic/lib/js/.",
    "www:js:lib:react-intl": "cp -f ./node_modules/react-intl/dist/react-intl.js ./node_modules/react-intl/dist/react-intl.js.map ./dist/classic/lib/js/.",
    "www:js:lib:flummox": "cp -f ./node_modules/flummox/dist/flummox.js ./dist/classic/lib/js/.",
    "www:js:lib:flummox:component": "browserify -o ./dist/classic/lib/js/flummox-component.js -e ./node_modules/flummox/lib/addons/FluxComponent.js -t browserify-shim -s FluxComponent",
    "www:js:lib:mnmo:shared": "browserify -o ./dist/classic/lib/js/component-shared.js -e node_modules/mnmo-components/lib/themes/mnmo/styles/shared.js -t browserify-shim -s MNMOVariables",
    "www:js:lib:entrypoints": "browserify -o ./dist/classic/js/entrypoints.js -e ./config/secret.entrypoints.json -t browserify-shim -s ConfigEntrypoints",
    "www:js:lib:mnmo": "babel-node ./bin/exportComponents.js",
    "www:js:polyfill:es5-shim": "cp node_modules/es5-shim/es5-shim.js ./dist/classic/lib/js/.",
    "www:js:polyfill:es5-sham": "cp node_modules/es5-shim/es5-sham.js ./dist/classic/lib/js/.",
    "www:js:polyfill:console": "cp node_modules/console-polyfill/index.js ./dist/classic/lib/js/console-polyfill.js",
    "www:js:polyfill:Intl": "cp ./node_modules/intl/Intl.js ./dist/classic/lib/js/.",
    "www:js:polyfill:fetch": "cp ./node_modules/whatwg-fetch/fetch.js ./dist/classic/lib/js/.",
    "www:js:app": "browserify -e ./src/app.js -o ./dist/classic/js/app.js -t babelify -t browserify-shim",
    "www:img:lib": "cp -R ./node_modules/mnmo-components/lib/themes/mnmo/img ./dist/classic/",
    "www:img:custom": "cp ./assets/custom/classic/bg01.jpg ./dist/classic/img/.",
    "www:html:index": "babel-node ./bin/page.js > ./dist/classic/index.html",
    "www:css:normalize": "cp -f ./node_modules/normalize.css/normalize.css ./dist/classic/lib/css/.",
    "www:css:typography": "cp -f ./node_modules/mnmo-components/lib/themes/mnmo/styles/typography.css ./dist/classic/lib/css/.",
    "www:css:app": "cp -f ./src/styles/main.css ./dist/classic/css/.",
    "www:fonts": "cp -f ./assets/fonts/Roboto/Roboto-Bold.ttf ./assets/fonts/Roboto/Roboto-Bold.woff ./assets/fonts/Roboto/Roboto-Light.ttf ./assets/fonts/Roboto/Roboto-Light.woff ./assets/fonts/Roboto/Roboto.eot  ./assets/fonts/Roboto/Roboto.svg  ./assets/fonts/Roboto/Roboto.ttf  ./assets/fonts/Roboto/Roboto.woff ./dist/classic/lib/fonts/.",
    "www:js:polyfill": "npm run www:js:polyfill:fetch && npm run www:js:polyfill:es5-shim && npm run www:js:polyfill:es5-sham && npm run www:js:polyfill:console && npm run www:js:polyfill:Intl",
    "www:js:lib": "npm run www:js:lib:react && npm run www:js:lib:react-intl && npm run www:js:lib:flummox && npm run www:js:lib:flummox:component && npm run www:js:lib:mnmo && npm run www:js:lib:mnmo:shared && npm run www:js:lib:entrypoints",
    "www:js": "npm run www:js:app && npm run www:js:lib && npm run www:js:polyfill",
    "www:img": "npm run www:img:lib && npm run www:img:custom",
    "www:css": "npm run www:css:normalize && npm run www:css:typography && npm run www:css:app",
    "www:html": "npm run www:html:index",
    "prewww": "npm run www:clean && npm run www:mkdir",
    "www": "npm run www:js && npm run www:html && npm run www:img && npm run www:css && npm run www:fonts",
    "classic:clean": "rm -rf ../monitor-traditional/lib ../monitor-traditional/js ../monitor-traditional/*.html",
    "classic:js:app": "cp -R ./dist/classic/js/app.js ../monitor-traditional/js/.",
    "classic:js:lib": "cp -R ./dist/classic/lib ../monitor-traditional/.",
    "classic:html": "cp -R ./dist/classic/*.html ../monitor-traditional/.",
    "classic:css": "cp -R ./dist/classic/css/* ../monitor-traditional/css/.",
    "classic:www": "cp -R ./dist/classic/* ../monitor-traditional/",
    "classic:docs:schedule": "babel-node ./bin/docSchedule.js > ../monitor-traditional/docs/schedule.html",
    "classic:ghpages:check": "cd ../monitor-traditional && git checkout gh-pages && git show master:docs/schedule.html > docs/schedule.html && git status && git checkout . && git checkout master",
    "classic:ghpages:update": "cd ../monitor-traditional && git checkout gh-pages && git show master:docs/schedule.html > docs/schedule.html && git add docs/* && git commit -m 'docs updated'  && git push origin gh-pages && git checkout master",
    "classic:docs": "npm run classic:docs:schedule",
    "preclassic:js:app": "npm run www:js:app",
    "preclassic:html": "npm run www:html",
    "preclassic:css": "npm run www:css",
    "preclassic:www": "npm run www",
    "preclassic": "npm run classic:clean",
    "classic": "npm run classic:www & npm run classic:docs",
    "start": "babel-node server.js",
    "test": "node ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fczuardi/mnmo-monitor.git"
  },
  "private": true,
  "keywords": [
    "webapp",
    "react",
    "javascript",
    "npm",
    "es6"
  ],
  "author": "Fabricio C Zuardi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fczuardi/mnmo-monitor/issues"
  },
  "homepage": "https://github.com/fczuardi/mnmo-monitor",
  "devDependencies": {
    "babel": "^4.7.16",
    "babelify": "^5.0.4",
    "browserify": "^9.0.3",
    "browserify-shim": "^3.8.3",
    "console-polyfill": "^0.2.1",
    "es5-shim": "^4.1.0",
    "eslint": "^0.17.1",
    "flummox": "^3.1.1",
    "goog-webfont-dl": "^0.1.1",
    "intl": "^0.1.4",
    "js-beautify": "^1.5.5",
    "koa": "^0.18.1",
    "koa-mount": "^1.3.0",
    "koa-router": "^4.2.0",
    "koa-static": "^1.4.9",
    "marked": "^0.3.3",
    "mnmo-components": "file:../mnmo-components/dist/npm",
    "normalize.css": "^3.0.2",
    "object-assign": "^2.0.0",
    "react": "^0.13.1",
    "react-intl": "^1.2.0",
    "whatwg-fetch": "^0.7.0"
  }
}
